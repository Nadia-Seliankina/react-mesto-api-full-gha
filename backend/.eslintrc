{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle":  ["error", { "allow": ["_id"] }],
    "import/prefer-default-export": [
      "off",
      { "target": "any" }
    ]
  }
}

//"off" or 0 - turn the rule off
